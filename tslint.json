{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": [
            false
        ],
        "member-access": [
            false
        ],
        "comment-format": [
            true,
            "check-space"
        ],
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "semicolon": [
            true,
            "never"
        ],
        "no-console": [
            false
        ],
        "no-var-requires": false
    },
    "rulesDirectory": []
}